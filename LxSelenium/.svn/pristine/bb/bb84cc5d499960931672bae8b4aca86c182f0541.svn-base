package com.lucernex.qa.pages;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

import java.util.List;

/**
 * IWMS Dashboard.
 */
public class DashboardPage {

    @FindBy(css = ".x-tool-expand-west")
    public WebElement expandModulesPanelButton;

    @FindBy(css = ".x-tool-collapse-west")
    public WebElement collapseModulesPanelButton;

    @FindBy(css = ".x-tree-node.anchor")
    public List<WebElement> dashboardModules;

    @FindBy(css = ".x-panel-body-noheader")
    public WebElement mainDashboardElement;


}
